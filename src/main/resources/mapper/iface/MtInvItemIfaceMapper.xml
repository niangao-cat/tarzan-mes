        item.VOLUME_UOM_ID,
        item.WEIGHT,
        item.WEIGHT_UOM_ID,
        item.SHELF_LIFE,
        item.SHELF_LIFE_UOM_ID,
        item.PRIMARY_UOM_ID,
        item.SECONDARY_UOM_ID,
        item.CONVERSION_RATE,
        item.ENABLE_FLAG,
        item.cid,
        item.OBJECT_VERSION_NUMBER,
        item.CREATED_BY,
        item.CREATION_DATE,
        item.LAST_UPDATED_BY,
        item.LAST_UPDATE_DATE
        FROM
        mt_material item
        INNER JOIN mt_material_tl tl on tl.MATERIAL_ID = item.MATERIAL_ID AND tl.LANG = #{lang}
        WHERE
        item.TENANT_ID = ${tenantId}
        <if test="materialList!= null">
            and ${materialList}
        </if>
    </select>
    <select id="getMateriaSiteReleationList" resultMap="resultView3" fetchSize="5000">
        select
        t.*
        from
        mt_site_plant_releation t
        where
        t.TENANT_ID = ${tenantId}
        <if test="materialList!=null and materialList.size>0">
            and t.PLANT_CODE in
            <foreach collection="materialList" item="i" separator="," open="(" close=")" index="index">
                #{i}
            </foreach>
        </if>
    </select>

    <select id="getMateriaSitelList" resultMap="resultView4">
        select
        t.*
        from
        MT_MATERIAL_SITE t
        where
