    <select id="getUomList" resultMap="resuleView" fetchSize="5000">
        <bind name="lang"
              value="@org.hzero.core.helper.LanguageHelper@language()"/>
        SELECT
        u.UOM_ID,
        u.UOM_TYPE,
        u.UOM_CODE,
        tl.UOM_NAME,
        u.PRIMARY_FLAG,
        u.CONVERSION_VALUE,
        u.DECIMAL_NUMBER,
        u.PROCESS_MODE,
        u.ENABLE_FLAG,
        u.CID,
        l1.DESCRIPTION UOM_TYPE_DESC,
        l2.DESCRIPTION PROCESS_MODE_DESC
        FROM
        mt_uom u
        LEFT JOIN mt_gen_type t1 ON t1.type_code = u.UOM_TYPE AND t1.type_group = 'UOM_TYPE'
        AND t1.TENANT_ID = ${tenantId}
        LEFT JOIN mt_gen_type_tl l1 ON l1.GEN_TYPE_ID = t1.gen_type_id
        AND l1.LANG =#{lang}
        LEFT JOIN mt_gen_type t2 ON t2.type_code = u.PROCESS_MODE
        AND t2.type_group = 'DECIMAL_PROCESS_MODE'
        AND t2.TENANT_ID = ${tenantId}
        LEFT JOIN mt_gen_type_tl l2 ON l2.GEN_TYPE_ID = t2.gen_type_id
        AND l2.LANG = #{lang}
        INNER JOIN mt_uom_tl tl ON u.UOM_ID = tl.UOM_ID
        AND tl.LANG = #{lang}
        WHERE
        u.TENANT_ID = ${tenantId}
        <if test="uomCodes!=null">
            and ${uomCodes}
        </if>
        <if test="uomCodes==null">
            and 1=-1
        </if>
    </select>
