        <if test="materialList!= null and materialList.size()>0">
            <foreach collection="materialList" item="item" separator="OR" open="and (" close=")" index="index">
                (
                t.MATERIAL_ID=#{item.materialId}
                and t.SITE_ID=#{item.siteId}
